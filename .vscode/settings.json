{
    "python.venvPath": "${env.VIRTUAL_ENV}",
    "jupyter.jupyterServerType": "local",
    "restructuredtext.builtDocumentationPath": "${workspaceFolder}/docs/_build/rst",
    "cSpell.words": [
        "Asser",
        "configfile",
        "elixr",
        "isit",
        "markdownlint",
        "pythonclass",
        "replit",
        "Spongebob",
        "Squarepants",
        "testpaths",
        "untakable",
        "wttr",
        "XANDER"
    ],
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "esbonio.server.enabled": false,
    "esbonio.sphinx.buildDir": "${workspaceFolder}/docs/_build/rst",
    "restructuredtext.languageServer.disabled": true
}
